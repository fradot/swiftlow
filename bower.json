{
  "name": "swiftlow",
  "description": "A notes editor for agile software developers.",
  "main": "app/app.js",
  "authors": [
    "Francesco Tucceri <fradot>"
  ],
  "license": "MIT",
  "keywords": [
    "angular",
    "notepad",
    "develper",
    "agile",
    "developer",
    "ES6"
  ],
  "homepage": "https://github.com/fradot/swiftlow",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "app_spec"
  ],
  "dependencies": {
    "bootstrap": "^3.3.7",
    "animate.css": "^3.5.2",
    "font-awesome": "fontawesome#^4.7.0",
    "tinymce": "^4.7.12"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/js/*.min.js",
        "dist/css/*.min.css",
        "dist/fonts/*"
      ]
    },
    "jquery": {
      "main": [
        "dist/jquery.min.js"
      ]
    },
    "animate.css": {
      "main": [
        "animate.min.css"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.min.css",
        "fonts/*"
      ]
    },
    "tinymce": {
      "main": [
        "tinymce.min.js",
        "jquery.tinymce.min.js",
        "plugins/{link,code}/*",
        "skins/lightgray/{fonts,img}/*",
        "skins/lightgray/{content.inline.min.css,content.min.css,skin.ie7.min.css,skin.min.css,skin.mobile.min.css}",
        "themes/{inlite,mobile,modern}/*"
      ]
    }
  }
}
